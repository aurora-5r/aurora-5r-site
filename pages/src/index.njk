<div class="site-section " id="questions-section2">
  <div class="container">
    <div class="row align-items-stretch justify-content-center">

      {% for offre in collections.offres %}
        <div class="col-md-4" data-aos="fade-up">
          <div class="unit-7 d-block">
            <div class="unit-7-icon mb-3">
              <span class="icon-wrap">
                <span class="text-primary"></span></span>
            </div>
            <h3 class="card-title">{{offre.data.title}}</h3>
            <p>
              <a href="{{offre.url}}">{{offre.data.question}}</a>
            </p>

          </div>
        </div>

        {% endfor%}
      </div>
    </div>
  </div>
