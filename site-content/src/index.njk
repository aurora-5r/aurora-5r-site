{#--NEWS  -------------------------------------------------------------------------------------#}

<div class="site-section " id="blog-section">
  <div class="container">
    <div class="row mt-2 mb-5">
      <div class="col-12 text-center">
        <h2 class="section-title mb-3">Actualit√©s</h2>
        <a href="/inscription" class="btn  btn-outline-dark">S'inscrire √† la newsletter pour ne rien rater</a>
      </div>
    </div>

    <div class="row ">

      {% set atPost = 0 %}
      {% set nb1 = collections.posts | length %}
      {% for post in collections.posts | reverse %}
        {% if atPost < 6 %}
          <div class="col-md-6 col-lg-4 mb-4 mb-lg-4">
            <div class="h-entry">
              <a href="{{post.url}}">
                <img class="small-img" src="{{post.data.cover}}" alt="illustration de {{post.title}}">
              </a>
              <h3>
                {{ post.data.title }}
              </h3>
              <div class="meta mb-4">{{ post.data.author }}
                <span class="mx-2">-</span>
                {{ post.date.toLocaleDateString('fr-FR') }}
              </div>
              <summary>
                {{ post.data.summary }}
              </summary>
              <p>
                <a href="{{post.url}}">Lire la suite...</a>
              </p>
            </div>
          </div>
          {% endif%}
          {% set atPost = atPost + 1 %}
        {% endfor %}

      </div>
    </div>
  </div>

</div>
{#--END NEWS  -------------------------------------------------------------------------------------#}

