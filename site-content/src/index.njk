{#--NEWS  -------------------------------------------------------------------------------------#}

<div class="site-section " id="blog-section">
  <div class="container">
    <div class="row mt-2 mb-5">
      <div class="col-12 text-center">
        <h2 class="section-title mb-3">Actualit√©s</h2>
        <a href="/inscription" class="btn  btn-outline-dark">S'inscrire √† la newsletter pour ne rien rater</a>
      </div>
    </div>
    <div class="container-fluid">
      <div class="row  justify-content-center">
        {% set atPost = 0 %}
        {% set nb1 = collections.posts | length %}
        {% for post in collections.posts %}
          {% if atPost < 3 %}
            <div class="card-deck mx-2" style="margin-bottom:3rem;">
              <div class="card unit-8" style="width:20rem;">
                <img class="card-img-top" src="{{post.url | url}}/images/{{post.data.cover}}" alt="illustration de {{post.title}}">
                <div class="card-body">
                  <h3 class="card-title">{{ post.data.title }}</h3>
                  <div class="meta">{{ post.data.author }}
                    <span class="mx-2">-</span>
                    {{ post.date.toLocaleDateString('fr-FR') }}
                  </div>

                  <p>
                    {{ post.data.summary }}
                  </p>

                  <p>
                    <a href="{{post.url |url }}">Lire la suite...</a>
                  </p>
                </div>
              </div>
            </div>
          {% endif %}
          {% set atPost = atPost + 1 %}
        {% endfor %}
      </div>
    </div>

  </div>
</div>

{#--END NEWS  -------------------------------------------------------------------------------------#}

{#--TEAM-------------------------------------------------------------------------------------#}
<div class="site-section" id="our-team-section">
  <div class="container">
    <div class="row mb-5 justify-content-center text-center" data-aos="fade-up">
      <div class="col-7 text-center  mb-5">
        <h2 class="section-title">L'√©quipe</h2>
        <p class="lead"></p>
      </div>
    </div>

    <div class="row">
      {% for bio in collections.bios %}
        <div class="col-lg-4 col-md-6 mb-4" data-aos="fade-up" data-aos-delay="100">
          <div class="person">
            <div class="bio-img">
              <figure><img src="{{ bio.url | url}}/images/{{bio.data.cover }}" alt="Photo de {{ bio.data.name }}" width="200px">
              </figure>
              <div class="social">
                <a href="{{ bio.data.linkedin |url }}">
                  <p class="hiddentext" style="display: none;">Linkedin de
                    {{ bio.data.name }}
                  </p>
                  <span class="icon-linkedin"></span>
                </a>

              </div>
            </div>
            <h2 class="text-black h1">{{ bio.data.name }}</h2>
            <span class="sub-title d-block mb-3">{{ bio.data.title }}</span>
            <p>{{ bio.data.bio }}</p>

          </div>
        </div>
      {% endfor %}

    </div>
  </div>
</div>
{#--END TEAM  -------------------------------------------------------------------------------------#}

