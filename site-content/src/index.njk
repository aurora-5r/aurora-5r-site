                  </a>

                  <h3>

                    {{ post.data.title }}
                  </h3>
                  <div class="meta mb-4">{{ post.data.author }}
                    <span class="mx-2">&bullet;</span>
                    {{ post.date.toLocaleDateString('fr-FR') }}

                  </div>
                  <summary>
                    {{ post.data.summary }}
                  </summary>
                  <p>
                    <a href="{{post.url}}">Lire la suite...</a>
                  </p>
                </div>
              </div>
              {% endif%}
              {% set atPost = atPost + 1 %}
            {% endfor %}

          </div>
        </div>
